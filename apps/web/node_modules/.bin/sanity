#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rc/github/glowing-engine/node_modules/.pnpm/sanity@3.92.0_@emotion+is-prop-valid@1.2.2_@types+node@20.19.0_@types+react@19.1.8_imme_b3fe4423b7ee8c21996706f8d1ee6912/node_modules/sanity/bin/node_modules:/Users/rc/github/glowing-engine/node_modules/.pnpm/sanity@3.92.0_@emotion+is-prop-valid@1.2.2_@types+node@20.19.0_@types+react@19.1.8_imme_b3fe4423b7ee8c21996706f8d1ee6912/node_modules/sanity/node_modules:/Users/rc/github/glowing-engine/node_modules/.pnpm/sanity@3.92.0_@emotion+is-prop-valid@1.2.2_@types+node@20.19.0_@types+react@19.1.8_imme_b3fe4423b7ee8c21996706f8d1ee6912/node_modules:/Users/rc/github/glowing-engine/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/rc/github/glowing-engine/node_modules/.pnpm/sanity@3.92.0_@emotion+is-prop-valid@1.2.2_@types+node@20.19.0_@types+react@19.1.8_imme_b3fe4423b7ee8c21996706f8d1ee6912/node_modules/sanity/bin/node_modules:/Users/rc/github/glowing-engine/node_modules/.pnpm/sanity@3.92.0_@emotion+is-prop-valid@1.2.2_@types+node@20.19.0_@types+react@19.1.8_imme_b3fe4423b7ee8c21996706f8d1ee6912/node_modules/sanity/node_modules:/Users/rc/github/glowing-engine/node_modules/.pnpm/sanity@3.92.0_@emotion+is-prop-valid@1.2.2_@types+node@20.19.0_@types+react@19.1.8_imme_b3fe4423b7ee8c21996706f8d1ee6912/node_modules:/Users/rc/github/glowing-engine/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/sanity@3.92.0_@emotion+is-prop-valid@1.2.2_@types+node@20.19.0_@types+react@19.1.8_imme_b3fe4423b7ee8c21996706f8d1ee6912/node_modules/sanity/bin/sanity" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/sanity@3.92.0_@emotion+is-prop-valid@1.2.2_@types+node@20.19.0_@types+react@19.1.8_imme_b3fe4423b7ee8c21996706f8d1ee6912/node_modules/sanity/bin/sanity" "$@"
fi
